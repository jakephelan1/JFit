<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="287.0" prefWidth="268.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.jf.Login">
   <children>
      <Label layoutX="80.0" layoutY="59.0" text="Login User">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="50.0" layoutY="164.0" promptText="Username" />
      <Label layoutX="50.0" layoutY="310.0" text="Not registered?">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Button layoutX="99.0" layoutY="261.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Hyperlink fx:id="createAccount" layoutX="121.0" layoutY="307.0" onAction="#createAccount" text="Create an account">
         <font>
            <Font size="11.0" />
         </font>
      </Hyperlink>
      <Label fx:id="err" layoutX="37.0" layoutY="238.5" textFill="RED" />
      <PasswordField fx:id="password" layoutX="50.0" layoutY="213.0" promptText="Password" />
   </children>
</AnchorPane>
